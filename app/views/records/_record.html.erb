<tr>
  <td><%= record.user.cell if record.user %></td>
  <td><%= Rumoji.decode(record.user.name) if record.user %></td>
  <td><%= record.city %></td>
  <td><%= record.prize %></td>
  <td><%= record.code %></td>
  <td><%= record.state %></td>
  <td><%= record.created_at.to_s(:db) %></td>
  <td><%= record.updated_at.to_s(:db) %></td>
  <td><%= link_to t(:receive), receive_record_path(record) %></td>
  <td><%= link_to t(:edit), edit_record_path(record) %></td>
  <td><%= link_to t(:destroy), record, method: :delete, data: {confirm: t(:are_you_sure)} %></td>
</tr>