%tr
  %td= record.user.cell if record.user
  %td= Rumoji.decode(record.user.name) if record.user
  %td= record.city
  %td= record.prize
  %td= record.code
  %td= record.state
  %td= record.created_at.to_s(:db)
  %td= record.updated_at.to_s(:db)
  %td= link_to t(:receive), receive_record_path(record)
  %td= link_to t(:edit), edit_record_path(record)
  %td= link_to t(:destroy), record, method: :delete, data: {confirm: t(:are_you_sure)}
