<div id="myModal<%=product.id%>" class="modal fade" tabindex="-1" role="dialog">
  <div class="modal-dialog modal-sm" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title text-center" id="myModalLabel">产品认证-预览</h4>
      </div>
      <div class="modal-body">
        <div class="page1" style="background-color: #1c1403; overflow: hidden;">
          <div id="divHeader" class="aboveBlock fl PB5 clearfix" >
              <img class="bg" src="/img/page1/1.png" />
              <div class="divChange">
                  <div class="stamp">
                      <img id="stamp"  src="/img/page1/page-1_tuzhang.png" />
                  </div>
                  <div class="divlogo">
                      <img id="divLogo" src="<%= product.product_logo %>" />
                  </div>
                  <div class="divTitle">
                      <span>亲爱的用户:</span>
                      <span id="msg" class="msg">经过专业扫码验证，您购买的此款<%= product.product_name %>是品牌正品。请放心使用。</span>
                  </div>

              </div>
          </div>

          <div class="midBlock fl clearfix">
              <input type="tel" placeholder="请输入您的手机号码"/>
              <a class="fr enter"><span>获取验证码</span></a>
          </div>
<!--
          <div class="underBlock fl">
             <img id="imgIntro"/>
          </div> -->
          <div class="underBlock fl MT5 clearfix">
              <img class="bg" src="<%= product.product_image %>">
              <!-- <img class="word" src="img/page1/page1_word.png"> -->
              <!-- <img class="head" src="img/page1/page1_head.png"> -->
              <!-- <img class="addr" src="img/page1/page1_addr.png"> -->
              <!-- <img class="logo" src="img/page1/page1_logo2.png"> -->
          </div>

        </div>
      </div>
    </div>
  </div>
</div>
