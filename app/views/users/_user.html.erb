<tr>
  <td><%= user.id %></td>
  <td><%= user.openid %></td>
  <td><%= Rumoji.decode(user.name) if user.name %></td>
  <td><%= user.cell %></td>
  <td><%= link_to t(:scanlog), user_scanlogs_path(user) if can? :read, User %></td>
  <td><%= link_to t(:edit), edit_user_path(user) if can? :edit, User %></td>
  <td><%= link_to t(:destroy), user, method: :delete, data: {confirm: t(:are_you_sure)} if can? :destroy, User  %></td>
</tr>
